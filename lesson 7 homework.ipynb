    "import turtle\n",
    "\n",
    "t = turtle.Turtle()\n",
    "\n",
    "def draw_triangle(side_length):\n",
    "    for i in range(0,50):\n",
    "        t.forward(side_length)\n",
    "        t.right(120)\n",
    "        t.right (5)\n",
    "draw_triangle(100)\n",
    "turtle.mainloop()"
   ]
